-include $(DIR)/Makefile

REAL_OBJ := $(patsubst %,$(DIR)/%,$(OBJ))
REAL_DEP := $(REAL_OBJ:.o=.d)
REAL_BUILTIN := $(DIR)/built-in.o

__clean: .FORCE
	@echo "  RM      $(REAL_BUILTIN)"
	$(Q)rm -f $(REAL_BUILTIN)
	@echo "  RM      $(REAL_OBJ)"
	$(Q)rm -f $(REAL_OBJ)
	@echo "  RM      $(REAL_DEP)"
	$(Q)rm -f $(REAL_DEP)

.PHONY: .FORCE
